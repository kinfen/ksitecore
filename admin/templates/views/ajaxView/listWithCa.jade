//include ../includes/kadm
include ../includes/kadmEx
section.content-header
	h1
		| Dashboard
		small Control panel
	ol.breadcrumb
		li
			a(href='#')
				i.fa.fa-dashboard
				|  Home
		li.active Dashboard
// Main content
section.content
	.row
		.col-sm-3
			#category
		.col-sm-9
			.box.box-primary
				.box-header.with-border
					h3.box-title 栏目组
					.box-tools
						button(data-widget="collapse").btn.btn-box-tool
							i.fa.fa-minus

				.box-body
					.list-tool-bar
						.action-sheet.dropdown.btn-group(title="Actions")
							button.btn.btn-default.dropdown-toggle(type="button", data-toggle="dropdown", aria-haspopup="true", aria-expanded="true")
								span.glyphicon.glyphicon-th-list
								span.caret
							ul.dropdown-menu(role="menu")
								li.edit.disabled
									a(type="button")
										span.glyphicon.glyphicon-edit
										label Edit
								li.delete
									a(type="button")
										span.glyphicon.glyphicon-trash
										label Remove
					table#table
						
script(src="#{adminPathEx}/dist/js/bootstrap-table.js")
script(src="#{adminPathEx}/dist/js/bootstrap-treeview.min.js")
script(src="#{adminPathEx}/script/Category.js")
script(src="#{adminPathEx}/script/inner.js")


